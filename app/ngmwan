config interface 'nwconf1'
#hello world 
	option ifname 'nw1'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option mode 'server'
	option bindport '10009'
	option log 'yes'
	option peer1 'p1,192.168.2.1,10010'

config interface 'nwconf2'
	option ifname 'nw2'
	option proto 'static'
	option ipaddr '10.10.10.7'
	option netmask '255.255.255.0'
	option mode 'client'
	option bindport '10009'
	option log 'yes'
	option peer1 'p3,192.168.1.5,10008'
	option peer2 'p4,192.168.1.5,10009'
config interface 'nwconf3'
	option ifname 'nw3'
	option proto 'static'
	option ipaddr '172.19.1.184'
	option netmask '255.255.255.0'
	option mode 'server'
	option bindport '23423'
	option log 'no'