config interface 'nw1'
      option ifname 'nw1'
      option proto 'static'
      option ipaddr '10.10.10.15'
      option netmask '255.255.255.0'
      option mode 'client'
      option oneclient 'no'
      option bindport '10020'
      option log 'no'
      option budget '256'
      option ownid 'p2'
      #multipath on or off only
      option multipath 'off'
      option peer 'p1,192.168.1.35,81'
      option peer 'p2,192.168.1.36,82'
      option peer 'p3,192.168.1.37,83'
config interface 'nw2'
      option ifname 'nw2'
      option proto 'static'
      option ipaddr '10.10.10.2'
      option netmask '255.255.255.0'
      option mode 'client'
      option oneclient 'yes'
      option bindport '10010'
      option log 'no'
      option budget '123'
      option ownid 'p3'
      #multipath on or off only
      option multipath 'on'
      option peer 'p6,192.168.1.35,83'
      option peer 'p4,192.168.1.36,85'
      option peer 'p5,192.168.1.37,88'
config interface 'nw3'
      option ifname 'nw3'
      option proto 'static'
      option ipaddr '10.10.10.5'
      option netmask '255.255.255.0'
      option mode 'server'
      option oneclient 'no'
      option bindport '10008'
