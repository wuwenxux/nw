config interface 'nw1'
      option ifname 'nw1'
      option proto 'static'
      option ipaddr '10.10.10.21'
      option netmask '255.255.255.0'
      option oneclient 'yes'
      option timeout '600'
      option bindport '1002'
      option queuelen '2300'
      option mode 'client'
      option log 'no'
      option budget '64'
      option interval '100'
      option ownid 'ppppp2'
      option multipath 'on'
      #multipath on or off only
      option peer 'p66666666666666666666666666,192.168.2.35,84'
      option peer 'p4,192.168.2.36,85'
      option peer 'p5,192.168.2.37,86'
      option peer 'p6,192.168.2.38,87'
      option peer 'p7,192.168.2.39,89'
      option peer 'p8,192.168.2.40,90'
      option peer 'p9,192.168.2.41,91'
      option peer 'p10,192.168.2.42,92'
      option peer 'p11,192.168.2.43,93'
      option peer 'p12,192.168.2.44,94'
      option dhcp 'yes'
      option dhcp-startip '192.168.1.1'
      option dhcp-endip '192.168.1.129'
      option dhcp-mask '255.255.224.0'

config interface 'nw2'
      option ifname 'nw2'
      option proto 'static'
