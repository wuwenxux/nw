config interface 'nwconf1'
#hello world 
	option ifname 'nw1'
	option proto 'dhcp'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option mode 'server'
	option bindport '10009'
	option log 'yes'
	option peer 'p1,192.168.2.3,10010'
	option peer 'p2,192.168.2.4,10011'

config interface 'nwconf2'
	option ifname 'nw2'
	option proto 'dhcp'
	option ipaddr '10.10.10.7'
	option netmask '255.255.255.0'
	option mode 'client'
	option bindport '10009'
	option log 'yes'
	option peer 'p1,192.168.1.1,10008'
	option peer 'p2,192.168.1.2,10008'
	
config interface 'nwconf3'
	option ifname 'nw3'
	option proto 'dhcp'
	option netmask '255.255.255.0'
	option mode 'server'
	option bindport '23423'
	option log 'no'