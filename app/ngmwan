config interface 'nw1'
	option ifname 'nw1'
  	option proto 'static'
  	option ipaddr '10.10.10.5'
  	option netmask '255.255.255.0'
  	option mode 'server'
  	option oneclient 'no'
  	option bindport '10008'
  	option log 'yes'
  	option budget '300'
	option ownid 'ppp1'
  	option peer 'p1,192.168.1.35,81'
  	option peer 'p2,192.168.1.36,82'
  	option peer 'p3,192.168.1.37,83'

config interface 'nw2'
        option ifname 'nw2'
        option proto 'static'
        option ipaddr '10.10.11.6'
        option netmask '255.255.255.0'
        option mode 'server'
        option oneclient 'no'
        option bindport '10009'
        option log 'no'
        option budget '100'
        #option ownid 'ppp2'
 		option multipath 'off'
        option peer 'p1,192.168.1.35,81'
        option peer 'p2,192.168.1.36,82'
        option peer 'p3,192.168.1.37,83'
  		option peer 'p4,0.0.0.0,0'

config interface 'nw23'
        option ifname 'nw3'
        option proto 'static'
        option ipaddr '10.10.12.7'
        option netmask '255.255.255.0'
        option mode 'client'
        option bindport '11009'
        option log 'no'
        option budget '200'
        #option ownid 'ppp2'
        option multipath 'off'
  		option bufflen '1024'
 		option queuelen '8192'
  		option idletimeout '300'
  		option batch '30'
  		option interval '500'
  		option timeout '3000'
        option peer 'p1,192.168.1.35,81'
        option peer 'p2,192.168.1.36,82'